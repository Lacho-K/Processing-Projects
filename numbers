void setup(){
size(600,600);
}

  int a = doubleNum(12);
  int b = doubleNum(34);
  int v = maxNum(22039,202230);

  
  

  int doubleNum(int num){
  return(2*num);}
  

  int maxNum(int num1, int num2){
  if(num1>num2){
  return(num1);
  }
  else{
  return (num2);}
}

void draw(){
println(a,b);
println(v);
}
